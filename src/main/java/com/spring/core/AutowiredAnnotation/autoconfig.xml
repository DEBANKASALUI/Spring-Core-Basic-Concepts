<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<!--This Autowiring byAnnotation -->
	<bean class="com.spring.core.AutowiredAnnotation.Address"
		name="tempaddress">
		<property name="street" value="Cannaught Palace" />
		<property name="city" value="Delhi" />
	</bean>
	<bean class="com.spring.core.AutowiredAnnotation.Address"
		name="temp1address">
		<property name="street" value="Mughal Garden" />
		<property name="city" value="Kashmir" />
	</bean>
	<bean class="com.spring.core.AutowiredAnnotation.Address"
		name="address">
		<property name="street" value="Dal Lake" />
		<property name="city" value="Jammu" />
	</bean>
	
	<bean class="com.spring.core.AutowiredAnnotation.Employee" name="emp1" />
	
<!-- 	<bean class="com.spring.core.AutowiredAnnotation.Employee" name="emp2" /> -->

</beans>